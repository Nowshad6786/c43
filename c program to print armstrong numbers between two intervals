#include <stdio.h>
int main() {
    int low, high, num, originalNum;
    int digit, count, sum, i, power;
    printf("Enter two numbers (intervals): ");
    scanf("%d %d", &low, &high);
    printf("Armstrong numbers between %d and %d are:\n", low, high);
    for (num = low; num <= high; num++) {
        originalNum = num;
        count = 0;
        sum = 0;
        while (originalNum != 0) {
            originalNum = originalNum / 10;
            count++;
        }
        originalNum = num;
        while (originalNum != 0) {
            digit = originalNum % 10;
            power = 1;
            for (i = 1; i <= count; i++) {
                power = power * digit;
            }
            sum = sum + power;
            originalNum = originalNum / 10;
        }
        if (sum == num) {
            printf("%d ", num);
        }
    }
    return 0;
}
